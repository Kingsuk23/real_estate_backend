generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model users {
  id String @id @default(uuid())
  role Roles
  name String
  email String @unique
  password String
  phone String
  licenseNumber String
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

enum Roles {
USER
AGENT
ADMIN
}